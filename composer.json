{
    "name": "webhubworks/weclapp-api-laravel",
    "description": "A Laravel wrapper around webhubworks/weclapp-api-core.",
    "keywords": [
        "Webhub",
        "laravel",
        "weclapp-api-laravel"
    ],
    "homepage": "https://github.com/webhubworks/weclapp-api-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Marven Thieme",
            "email": "thieme@webhub.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/contracts": "^10.0||^11.0||^12.0",
        "spatie/laravel-package-tools": "^1.16",
        "webhubworks/weclapp-api-core": "3.0.1"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "orchestra/testbench": "^10.0.0||^9.0.0||^8.22.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "spatie/laravel-ray": "^1.35"
    },
    "autoload": {
        "psr-4": {
            "Webhub\\WeclappApiLaravel\\": "src/",
            "Webhub\\WeclappApiLaravel\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webhub\\WeclappApiLaravel\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webhub\\WeclappApiLaravel\\WeclappApiLaravelServiceProvider"
            ],
            "aliases": {
                "Weclapp": "Webhub\\WeclappApiLaravel\\Facades\\Weclapp"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
